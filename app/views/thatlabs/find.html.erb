<nav class="navbar navbar-light bg-light">
  <a class="navbar-brand">Find by Lab</a>
  <form class="form-inline" action="/thatlab" method= "get">
    <input class="form-control mr-sm-2" name="search" type="search" placeholder="Search" aria-label="Search">
    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
  </form>
</nav>
<br>
<br>


<% if @questions == nil %>
  <% redirect_to error_path %>
  <% else %>
  <% @questions.each do |question| %>
    <br>
    <ul>
      <li>
    <div class="span8">
      <h1><a href="/questions/<%= question.id %>"><%= question.question_title %></a></h1>
      <strong><%= question.lab %></strong>
      <p><%= question.question_content %></p>
      <div>
          <span class="badge badge-success">Answered by:<%= question.user.username %></span><div class="pull-right"><span class="label"><%= question.lab %></span></div>
      </div>
      <hr>
    </div>
    </li>
    </ul>
  <br>
  <% end %>
  <% end %>
