<section class="section">
  <div class="container ">
    <h1 class="title">
      <%= @note.title%>
    </h1>
    <div class="content">
      <%= @note.content%>
    </div>
  </div>
    <%= link_to "Edit", edit_note_path(@note), class:"button" %>
    <%= link_to "Delete", note_path(@note), method: :delete, data: {confirm: "Aare you sure?"}, class: "button is-danger" %>
    <%= link_to "New Note", new_note_path, class:"button" %>

</section>
